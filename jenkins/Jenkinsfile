node {
  try {
    stage('Checkout') {
      checkout scm
    }
    stage('Environment') {
      sh 'git --version'
      echo "Branch: ${env.BRANCH_NAME}"
      sh 'docker -v'
      sh 'printenv'
    }
    stage('Build Docker test'){
      sh 'minikube image build -t fipewatcher:latest . '
    }
    stage('Deploy'){
      if(env.BRANCH_NAME == 'master'){
        sh 'minikube image build -t fipewatcher:latest '
      }
    }
  }
  catch (err) {
    throw err
  }
}